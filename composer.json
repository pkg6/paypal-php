{
  "name": "pkg6/paypal",
  "license": "MIT",
  "description": "php client for PayPal SDK",
  "keywords": [
    "paypal",
    "REST API"
  ],
  "authors": [
    {
      "name": "zhiqiang",
      "email": "zhiqiang2033@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6|^7",
    "psr/simple-cache": "^1.0|^2.0|^3.0",
    "nesbot/carbon": "^1.0|^2.0|^3.0",
    "symfony/http-foundation": "^5.0|^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "pkg6\\paypal\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.9",
    "mockery/mockery": ">=0.9.9",
    "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0",
    "jaeger/querylist": "^4.2"
  },
  "scripts": {
    "spider-data": "php bin/spider_data"
  },
  "autoload-dev": {
    "psr-4": {
      "pkg6\\PaypalRest\\Tests\\": "tests/"
    }
  }
}
